<p id="notice"><%= notice %></p>

<h1>Tube Vids</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tube_vids.each do |tube_vid| %>
      <tr>
        <td><%= link_to 'Show', tube_vid %></td>
        <td><%= link_to 'Edit', edit_tube_vid_path(tube_vid) %></td>
        <td><%= link_to 'Destroy', tube_vid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tube Vid', new_tube_vid_path %>
